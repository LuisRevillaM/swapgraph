{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/PartnerProgramCommercialUsageRecordResponse.schema.json",
  "title": "PartnerProgramCommercialUsageRecordResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "entry",
    "ledger_summary",
    "replayed"
  ],
  "properties": {
    "correlation_id": {
      "type": "string",
      "minLength": 1
    },
    "entry": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "entry_id",
        "partner_id",
        "feature_code",
        "unit_type",
        "units",
        "unit_price_usd_micros",
        "amount_usd_micros",
        "occurred_at",
        "metadata"
      ],
      "properties": {
        "entry_id": {
          "type": "string",
          "minLength": 1
        },
        "partner_id": {
          "type": "string",
          "minLength": 1
        },
        "feature_code": {
          "type": "string",
          "minLength": 1
        },
        "unit_type": {
          "type": "string",
          "minLength": 1
        },
        "units": {
          "type": "integer",
          "minimum": 0
        },
        "unit_price_usd_micros": {
          "type": "integer",
          "minimum": 0
        },
        "amount_usd_micros": {
          "type": "integer",
          "minimum": 0
        },
        "occurred_at": {
          "type": "string",
          "format": "date-time"
        },
        "metadata": {
          "type": "object"
        }
      }
    },
    "ledger_summary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "partner_id",
        "entries_count",
        "total_units",
        "total_amount_usd_micros",
        "feature_breakdown"
      ],
      "properties": {
        "partner_id": {
          "type": "string",
          "minLength": 1
        },
        "entries_count": {
          "type": "integer",
          "minimum": 0
        },
        "total_units": {
          "type": "integer",
          "minimum": 0
        },
        "total_amount_usd_micros": {
          "type": "integer",
          "minimum": 0
        },
        "feature_breakdown": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "feature_code",
              "unit_type",
              "units",
              "amount_usd_micros"
            ],
            "properties": {
              "feature_code": {
                "type": "string",
                "minLength": 1
              },
              "unit_type": {
                "type": "string",
                "minLength": 1
              },
              "units": {
                "type": "integer",
                "minimum": 0
              },
              "amount_usd_micros": {
                "type": "integer",
                "minimum": 0
              }
            }
          }
        }
      }
    },
    "replayed": {
      "type": "boolean"
    }
  }
}
