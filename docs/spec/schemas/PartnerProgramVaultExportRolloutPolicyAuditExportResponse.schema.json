{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/PartnerProgramVaultExportRolloutPolicyAuditExportResponse.schema.json",
  "title": "PartnerProgramVaultExportRolloutPolicyAuditExportResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "exported_at",
    "query",
    "policy",
    "total_filtered",
    "entries",
    "export_hash",
    "signature"
  ],
  "properties": {
    "correlation_id": { "type": "string", "minLength": 1 },
    "exported_at": { "type": "string", "format": "date-time" },
    "query": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "from_iso": { "type": "string", "format": "date-time" },
        "to_iso": { "type": "string", "format": "date-time" },
        "now_iso": { "type": "string", "format": "date-time" },
        "exported_at_iso": { "type": "string", "format": "date-time" },
        "limit": { "type": "integer", "minimum": 1, "maximum": 200 },
        "cursor_after": { "type": "string", "pattern": "^rollout_policy_[0-9]{6}$" },
        "attestation_after": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "checkpoint_after": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "policy": { "$ref": "PartnerProgramVaultExportRolloutPolicyView.schema.json" },
    "total_filtered": { "type": "integer", "minimum": 0 },
    "entries": {
      "type": "array",
      "items": { "$ref": "PartnerProgramVaultExportRolloutPolicyAuditEntry.schema.json" }
    },
    "next_cursor": { "type": "string", "pattern": "^rollout_policy_[0-9]{6}$" },
    "attestation": {
      "type": "object",
      "additionalProperties": false,
      "required": ["cursor_after", "next_cursor", "attestation_after", "page_hash", "chain_hash"],
      "properties": {
        "cursor_after": {
          "type": ["string", "null"],
          "pattern": "^rollout_policy_[0-9]{6}$"
        },
        "next_cursor": {
          "type": ["string", "null"],
          "pattern": "^rollout_policy_[0-9]{6}$"
        },
        "attestation_after": {
          "type": ["string", "null"],
          "pattern": "^[a-f0-9]{64}$"
        },
        "page_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "chain_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "checkpoint": {
      "type": "object",
      "additionalProperties": false,
      "required": ["checkpoint_after", "attestation_chain_hash", "next_cursor", "entries_count", "total_filtered", "checkpoint_hash"],
      "properties": {
        "checkpoint_after": { "type": ["string", "null"], "pattern": "^[a-f0-9]{64}$" },
        "attestation_chain_hash": { "type": ["string", "null"], "pattern": "^[a-f0-9]{64}$" },
        "next_cursor": {
          "type": ["string", "null"],
          "pattern": "^rollout_policy_[0-9]{6}$"
        },
        "entries_count": { "type": "integer", "minimum": 0 },
        "total_filtered": { "type": "integer", "minimum": 0 },
        "checkpoint_hash": { "type": "string", "pattern": "^[a-f0-9]{64}$" }
      }
    },
    "export_hash": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$"
    },
    "signature": { "$ref": "EventSignature.schema.json" }
  }
}
