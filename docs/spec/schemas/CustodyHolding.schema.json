{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/CustodyHolding.schema.json",
  "title": "CustodyHolding",
  "type": "object",
  "additionalProperties": false,
  "required": ["holding_id", "asset", "owner_actor", "vault_id", "deposit_id", "deposited_at"],
  "properties": {
    "holding_id": { "type": "string", "minLength": 1 },
    "asset": { "$ref": "AssetRef.schema.json" },
    "owner_actor": { "$ref": "ActorRef.schema.json" },
    "vault_id": { "type": ["string", "null"], "minLength": 1 },
    "deposit_id": { "type": ["string", "null"], "minLength": 1 },
    "deposited_at": { "type": ["string", "null"], "format": "date-time" }
  }
}
