{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/PolicyAuditListResponse.schema.json",
  "title": "PolicyAuditListResponse",
  "type": "object",
  "additionalProperties": false,
  "required": ["correlation_id", "entries"],
  "properties": {
    "correlation_id": { "type": "string", "minLength": 1 },
    "entries": {
      "type": "array",
      "items": { "$ref": "PolicyAuditEntry.schema.json" }
    },
    "next_cursor": { "type": "string", "minLength": 1 },
    "total_filtered": { "type": "integer", "minimum": 0 }
  }
}
