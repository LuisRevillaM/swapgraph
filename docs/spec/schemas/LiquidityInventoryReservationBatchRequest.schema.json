{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/LiquidityInventoryReservationBatchRequest.schema.json",
  "title": "LiquidityInventoryReservationBatchRequest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "reservations"
  ],
  "properties": {
    "reservations": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "asset_id",
          "quantity",
          "cycle_id"
        ],
        "properties": {
          "reservation_id": {
            "type": "string",
            "minLength": 1
          },
          "holding_id": {
            "type": "string",
            "minLength": 1
          },
          "asset_id": {
            "type": "string",
            "minLength": 1
          },
          "platform": {
            "type": "string",
            "minLength": 1
          },
          "quantity": {
            "type": "integer",
            "minimum": 1
          },
          "cycle_id": {
            "type": "string",
            "minLength": 1
          },
          "context_ref": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    },
    "recorded_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
