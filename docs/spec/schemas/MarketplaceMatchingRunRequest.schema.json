{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/MarketplaceMatchingRunRequest.schema.json",
  "title": "MarketplaceMatchingRunRequest",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "replace_existing": {
      "type": "boolean"
    },
    "max_proposals": {
      "type": "integer",
      "minimum": 1,
      "maximum": 200
    },
    "recorded_at": {
      "type": "string",
      "format": "date-time"
    },
    "asset_values_usd": {
      "type": "object",
      "additionalProperties": {
        "type": "number",
        "minimum": 0
      }
    }
  }
}
