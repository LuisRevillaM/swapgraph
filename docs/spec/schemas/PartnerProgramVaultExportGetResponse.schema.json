{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/PartnerProgramVaultExportGetResponse.schema.json",
  "title": "PartnerProgramVaultExportGetResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "partner_program",
    "rollout_policy",
    "export_allowed",
    "reasons"
  ],
  "properties": {
    "correlation_id": { "type": "string", "minLength": 1 },
    "partner_program": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "partner_id",
        "plan_id",
        "feature_vault_reconciliation_export",
        "quota_day",
        "quota_daily_limit",
        "quota_daily_used"
      ],
      "properties": {
        "partner_id": { "type": "string", "minLength": 1 },
        "plan_id": { "type": ["string", "null"], "minLength": 1 },
        "feature_vault_reconciliation_export": { "type": "boolean" },
        "quota_day": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
        "quota_daily_limit": { "type": ["integer", "null"], "minimum": 1 },
        "quota_daily_used": { "type": "integer", "minimum": 0 }
      }
    },
    "rollout_policy": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "partner_program_enforced",
        "checkpoint_enforced",
        "checkpoint_retention_days",
        "allowlist_enforced",
        "partner_allowed",
        "min_plan_id",
        "plan_meets_minimum"
      ],
      "properties": {
        "partner_program_enforced": { "type": "boolean" },
        "checkpoint_enforced": { "type": "boolean" },
        "checkpoint_retention_days": { "type": "integer", "minimum": 1 },
        "allowlist_enforced": { "type": "boolean" },
        "partner_allowed": { "type": "boolean" },
        "min_plan_id": { "type": ["string", "null"], "enum": [null, "starter", "pro", "enterprise"] },
        "plan_meets_minimum": { "type": ["boolean", "null"] },
        "config_error": { "type": "string", "minLength": 1 }
      }
    },
    "export_allowed": { "type": "boolean" },
    "reasons": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "partner_program_missing",
          "partner_feature_not_enabled",
          "partner_quota_exceeded",
          "partner_rollout_not_allowed",
          "partner_plan_insufficient",
          "partner_rollout_config_invalid"
        ]
      }
    }
  }
}
