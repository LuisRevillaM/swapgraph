{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/SteamTier1DepositPerSwapLiveProofRecordResponse.schema.json",
  "title": "SteamTier1DepositPerSwapLiveProofRecordResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "proof",
    "replayed"
  ],
  "properties": {
    "correlation_id": {
      "type": "string",
      "minLength": 1
    },
    "proof": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "proof_id",
        "partner_id",
        "contract_version",
        "cycle_id",
        "settlement_id",
        "receipt_id",
        "steam_trade_offer_id",
        "steam_trade_status",
        "deposit_reference",
        "asset_count",
        "live_mode",
        "operator_ref",
        "evidence_refs",
        "integration_mode",
        "recorded_at",
        "proof_hash"
      ],
      "properties": {
        "proof_id": {
          "type": "string",
          "minLength": 1
        },
        "partner_id": {
          "type": "string",
          "minLength": 1
        },
        "contract_version": {
          "type": "integer",
          "minimum": 1
        },
        "cycle_id": {
          "type": "string",
          "minLength": 1
        },
        "settlement_id": {
          "type": "string",
          "minLength": 1
        },
        "preflight_id": {
          "type": "string",
          "minLength": 1
        },
        "receipt_id": {
          "type": "string",
          "minLength": 1
        },
        "steam_trade_offer_id": {
          "type": "string",
          "minLength": 1
        },
        "steam_trade_status": {
          "type": "string",
          "enum": [
            "accepted",
            "completed"
          ]
        },
        "deposit_reference": {
          "type": "string",
          "minLength": 1
        },
        "asset_count": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100000
        },
        "live_mode": {
          "type": "boolean"
        },
        "operator_ref": {
          "type": "string",
          "minLength": 1
        },
        "evidence_refs": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "notes": {
          "type": "string",
          "minLength": 1
        },
        "integration_mode": {
          "type": "string",
          "enum": [
            "live"
          ]
        },
        "recorded_at": {
          "type": "string",
          "format": "date-time"
        },
        "proof_hash": {
          "type": "string",
          "minLength": 64,
          "maxLength": 64
        }
      }
    },
    "replayed": {
      "type": "boolean"
    }
  }
}
