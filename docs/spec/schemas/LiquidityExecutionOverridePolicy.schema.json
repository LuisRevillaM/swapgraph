{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/LiquidityExecutionOverridePolicy.schema.json",
  "title": "LiquidityExecutionOverridePolicy",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "override_id",
    "approved_by",
    "approved_at",
    "reason_codes",
    "expires_at"
  ],
  "properties": {
    "override_id": {
      "type": "string",
      "minLength": 1
    },
    "approved_by": {
      "$ref": "ActorRef.schema.json"
    },
    "approved_at": {
      "type": "string",
      "format": "date-time"
    },
    "reason_codes": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "expires_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "minLength": 1
    }
  }
}
