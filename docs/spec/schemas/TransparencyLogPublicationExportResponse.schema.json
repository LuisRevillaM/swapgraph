{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/TransparencyLogPublicationExportResponse.schema.json",
  "title": "TransparencyLogPublicationExportResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "exported_at",
    "query",
    "summary",
    "publications",
    "total_filtered",
    "export_hash",
    "signature"
  ],
  "properties": {
    "correlation_id": {
      "type": "string",
      "minLength": 1
    },
    "exported_at": {
      "type": "string",
      "format": "date-time"
    },
    "query": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "source_type": {
          "type": "string",
          "enum": [
            "settlement_receipts",
            "governance_rollout",
            "partner_disputes",
            "combined"
          ]
        },
        "from_iso": {
          "type": "string",
          "format": "date-time"
        },
        "to_iso": {
          "type": "string",
          "format": "date-time"
        },
        "limit": {
          "type": "integer",
          "minimum": 1,
          "maximum": 200
        },
        "cursor_after": {
          "type": "string",
          "minLength": 1
        },
        "attestation_after": {
          "type": "string",
          "minLength": 1
        },
        "checkpoint_after": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "now_iso": {
          "type": "string",
          "format": "date-time"
        },
        "exported_at_iso": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "summary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "total_publications",
        "returned_count",
        "total_entries",
        "chain_head",
        "chain_tail"
      ],
      "properties": {
        "total_publications": {
          "type": "integer",
          "minimum": 0
        },
        "returned_count": {
          "type": "integer",
          "minimum": 0
        },
        "total_entries": {
          "type": "integer",
          "minimum": 0
        },
        "chain_head": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "chain_tail": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "publications": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "publication_id",
          "publication_index",
          "partner_id",
          "source_type",
          "source_ref",
          "root_hash",
          "previous_root_hash",
          "previous_chain_hash",
          "chain_hash",
          "entry_count",
          "artifact_refs",
          "linked_receipt_ids",
          "linked_governance_artifact_ids",
          "integration_mode",
          "published_at"
        ],
        "properties": {
          "publication_id": {
            "type": "string",
            "minLength": 1
          },
          "publication_index": {
            "type": "integer",
            "minimum": 1
          },
          "partner_id": {
            "type": "string",
            "minLength": 1
          },
          "source_type": {
            "type": "string",
            "enum": [
              "settlement_receipts",
              "governance_rollout",
              "partner_disputes",
              "combined"
            ]
          },
          "source_ref": {
            "type": "string",
            "minLength": 1
          },
          "root_hash": {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          "previous_root_hash": {
            "type": [
              "string",
              "null"
            ],
            "pattern": "^[a-f0-9]{64}$"
          },
          "previous_chain_hash": {
            "type": [
              "string",
              "null"
            ],
            "pattern": "^[a-f0-9]{64}$"
          },
          "chain_hash": {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          "entry_count": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1000000
          },
          "artifact_refs": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string",
              "minLength": 1
            }
          },
          "linked_receipt_ids": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            }
          },
          "linked_governance_artifact_ids": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            }
          },
          "notes": {
            "type": "string",
            "minLength": 1
          },
          "integration_mode": {
            "type": "string",
            "enum": [
              "fixture_only"
            ]
          },
          "published_at": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "total_filtered": {
      "type": "integer",
      "minimum": 0
    },
    "next_cursor": {
      "type": "string",
      "minLength": 1
    },
    "attestation": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "cursor_after",
        "next_cursor",
        "attestation_after",
        "page_hash",
        "chain_hash"
      ],
      "properties": {
        "cursor_after": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1
        },
        "next_cursor": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1
        },
        "attestation_after": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1
        },
        "page_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "chain_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "checkpoint": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "checkpoint_after",
        "attestation_chain_hash",
        "next_cursor",
        "entries_count",
        "total_filtered",
        "checkpoint_hash"
      ],
      "properties": {
        "checkpoint_after": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "attestation_chain_hash": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "next_cursor": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1
        },
        "entries_count": {
          "type": "integer",
          "minimum": 0
        },
        "total_filtered": {
          "type": "integer",
          "minimum": 0
        },
        "checkpoint_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "export_hash": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$"
    },
    "signature": {
      "$ref": "EventSignature.schema.json"
    }
  }
}
