{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/LiquidityInventoryReservationView.schema.json",
  "title": "LiquidityInventoryReservationView",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "reservation_id",
    "provider_id",
    "holding_id",
    "asset_id",
    "platform",
    "quantity",
    "cycle_id",
    "context_ref",
    "status",
    "owner_actor",
    "created_at",
    "updated_at",
    "released_at"
  ],
  "properties": {
    "reservation_id": {
      "type": "string",
      "minLength": 1
    },
    "provider_id": {
      "type": "string",
      "minLength": 1
    },
    "holding_id": {
      "type": "string",
      "minLength": 1
    },
    "asset_id": {
      "type": "string",
      "minLength": 1
    },
    "platform": {
      "type": "string",
      "minLength": 1
    },
    "quantity": {
      "type": "integer",
      "minimum": 1
    },
    "cycle_id": {
      "type": "string",
      "minLength": 1
    },
    "context_ref": {
      "type": [
        "string",
        "null"
      ],
      "minLength": 1
    },
    "status": {
      "type": "string",
      "enum": [
        "reserved",
        "in_settlement",
        "released",
        "refunded",
        "withdrawn"
      ]
    },
    "owner_actor": {
      "$ref": "ActorRef.schema.json"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "released_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    }
  }
}
