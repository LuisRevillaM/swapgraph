{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/LiquidityListingUpsertRequest.schema.json",
  "title": "LiquidityListingUpsertRequest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "listing"
  ],
  "properties": {
    "listing": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "intent",
        "policy_ref",
        "listed_at"
      ],
      "properties": {
        "intent": {
          "$ref": "SwapIntent.schema.json"
        },
        "policy_ref": {
          "$ref": "LiquidityPolicyRef.schema.json"
        },
        "listed_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  }
}
