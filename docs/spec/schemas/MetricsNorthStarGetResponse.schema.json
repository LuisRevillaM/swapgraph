{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/MetricsNorthStarGetResponse.schema.json",
  "title": "MetricsNorthStarGetResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "partner_id",
    "window",
    "summary"
  ],
  "properties": {
    "correlation_id": {
      "type": "string",
      "minLength": 1
    },
    "partner_id": {
      "type": "string",
      "minLength": 1
    },
    "window": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "from_iso",
        "to_iso",
        "bucket"
      ],
      "properties": {
        "from_iso": {
          "type": "string",
          "format": "date-time"
        },
        "to_iso": {
          "type": "string",
          "format": "date-time"
        },
        "bucket": {
          "type": "string",
          "enum": [
            "hour",
            "day",
            "week"
          ]
        }
      }
    },
    "summary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "weekly_successful_swaps_per_active_trader",
        "fill_rate_7d_bps",
        "proposal_to_accept_bps",
        "accept_to_complete_bps",
        "webhook_delivery_success_bps",
        "fraud_flags_per_1000_intents",
        "unwind_rate_bps",
        "weekly_successful_swaps_count_7d",
        "active_trader_count_7d",
        "fill_rate_7d_numerator_completed",
        "fill_rate_7d_denominator_terminal",
        "proposal_to_accept_numerator",
        "proposal_to_accept_denominator",
        "accept_to_complete_numerator",
        "accept_to_complete_denominator",
        "webhook_delivery_success_numerator",
        "webhook_delivery_success_denominator",
        "fraud_flags_numerator",
        "fraud_flags_denominator_intents",
        "unwind_numerator",
        "unwind_denominator_terminal",
        "primary_metric"
      ],
      "properties": {
        "weekly_successful_swaps_per_active_trader": {
          "type": "number",
          "minimum": 0
        },
        "fill_rate_7d_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "proposal_to_accept_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "accept_to_complete_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "webhook_delivery_success_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "fraud_flags_per_1000_intents": {
          "type": "integer",
          "minimum": 0
        },
        "unwind_rate_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "weekly_successful_swaps_count_7d": {
          "type": "integer",
          "minimum": 0
        },
        "active_trader_count_7d": {
          "type": "integer",
          "minimum": 0
        },
        "fill_rate_7d_numerator_completed": {
          "type": "integer",
          "minimum": 0
        },
        "fill_rate_7d_denominator_terminal": {
          "type": "integer",
          "minimum": 0
        },
        "proposal_to_accept_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "proposal_to_accept_denominator": {
          "type": "integer",
          "minimum": 0
        },
        "accept_to_complete_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "accept_to_complete_denominator": {
          "type": "integer",
          "minimum": 0
        },
        "webhook_delivery_success_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "webhook_delivery_success_denominator": {
          "type": "integer",
          "minimum": 0
        },
        "fraud_flags_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "fraud_flags_denominator_intents": {
          "type": "integer",
          "minimum": 0
        },
        "unwind_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "unwind_denominator_terminal": {
          "type": "integer",
          "minimum": 0
        },
        "primary_metric": {
          "type": "string",
          "enum": [
            "weekly_successful_swaps_per_active_trader"
          ]
        }
      }
    }
  }
}
