{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/VaultCustodySnapshotListResponse.schema.json",
  "title": "VaultCustodySnapshotListResponse",
  "type": "object",
  "additionalProperties": false,
  "required": ["correlation_id", "snapshots", "total"],
  "properties": {
    "correlation_id": { "type": "string", "minLength": 1 },
    "snapshots": {
      "type": "array",
      "items": { "$ref": "VaultCustodySnapshotSummary.schema.json" }
    },
    "next_cursor": { "type": "string", "minLength": 1 },
    "total": { "type": "integer", "minimum": 0 }
  }
}
