{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/PartnerProgramDisputeResolveRequest.schema.json",
  "title": "PartnerProgramDisputeResolveRequest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "resolution"
  ],
  "properties": {
    "dispute_id": {
      "type": "string",
      "minLength": 1
    },
    "resolved_at": {
      "type": "string",
      "format": "date-time"
    },
    "resolution": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "code"
      ],
      "properties": {
        "code": {
          "type": "string",
          "minLength": 1
        },
        "notes": {
          "type": "string",
          "minLength": 1
        }
      }
    }
  }
}
