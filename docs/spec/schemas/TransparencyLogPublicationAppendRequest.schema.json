{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/TransparencyLogPublicationAppendRequest.schema.json",
  "title": "TransparencyLogPublicationAppendRequest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "entries"
  ],
  "properties": {
    "entries": {
      "type": "array",
      "minItems": 1,
      "maxItems": 100,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "entry_type",
          "entity_id",
          "entity_hash"
        ],
        "properties": {
          "entry_type": {
            "type": "string",
            "enum": [
              "receipt",
              "governance_artifact"
            ]
          },
          "entity_id": {
            "type": "string",
            "minLength": 1
          },
          "entity_hash": {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          "cycle_id": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    },
    "occurred_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
