{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/CycleFailedPayload.schema.json",
  "title": "CycleFailedPayload",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "cycle_id",
    "from_state",
    "reason_code",
    "failed_at"
  ],
  "properties": {
    "cycle_id": {
      "type": "string",
      "minLength": 1
    },
    "from_state": {
      "type": "string",
      "minLength": 1
    },
    "reason_code": {
      "type": "string",
      "minLength": 1
    },
    "failed_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
