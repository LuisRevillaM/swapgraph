{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/MetricsPartnerHealthGetResponse.schema.json",
  "title": "MetricsPartnerHealthGetResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "partner_id",
    "window",
    "partner_health"
  ],
  "properties": {
    "correlation_id": {
      "type": "string",
      "minLength": 1
    },
    "partner_id": {
      "type": "string",
      "minLength": 1
    },
    "window": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "from_iso",
        "to_iso",
        "bucket"
      ],
      "properties": {
        "from_iso": {
          "type": "string",
          "format": "date-time"
        },
        "to_iso": {
          "type": "string",
          "format": "date-time"
        },
        "bucket": {
          "type": "string",
          "enum": [
            "hour",
            "day",
            "week"
          ]
        }
      }
    },
    "partner_health": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "proposal_delivery_success_bps",
        "proposal_delivery_success_numerator",
        "proposal_delivery_success_denominator",
        "commit_to_completion_bps",
        "commit_to_completion_numerator",
        "commit_to_completion_denominator",
        "webhook_delivery_success_bps",
        "webhook_delivery_success_numerator",
        "webhook_delivery_success_denominator",
        "webhook_dead_letter_rate_bps",
        "webhook_dead_letter_numerator",
        "webhook_dead_letter_denominator"
      ],
      "properties": {
        "proposal_delivery_success_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "proposal_delivery_success_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "proposal_delivery_success_denominator": {
          "type": "integer",
          "minimum": 0
        },
        "commit_to_completion_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "commit_to_completion_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "commit_to_completion_denominator": {
          "type": "integer",
          "minimum": 0
        },
        "webhook_delivery_success_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "webhook_delivery_success_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "webhook_delivery_success_denominator": {
          "type": "integer",
          "minimum": 0
        },
        "webhook_dead_letter_rate_bps": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "webhook_dead_letter_numerator": {
          "type": "integer",
          "minimum": 0
        },
        "webhook_dead_letter_denominator": {
          "type": "integer",
          "minimum": 0
        }
      }
    }
  }
}
