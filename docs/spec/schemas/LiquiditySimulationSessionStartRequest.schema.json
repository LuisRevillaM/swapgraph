{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/LiquiditySimulationSessionStartRequest.schema.json",
  "title": "LiquiditySimulationSessionStartRequest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "session"
  ],
  "properties": {
    "session": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "label"
      ],
      "properties": {
        "session_id": {
          "type": "string",
          "minLength": 1
        },
        "label": {
          "type": "string",
          "minLength": 1
        },
        "mode": {
          "type": "string",
          "const": "simulation"
        },
        "seed": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "started_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
