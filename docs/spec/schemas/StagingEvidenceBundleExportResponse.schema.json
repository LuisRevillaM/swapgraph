{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://swapgraph.dev/schemas/StagingEvidenceBundleExportResponse.schema.json",
  "title": "StagingEvidenceBundleExportResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "partner_id",
    "integration_mode",
    "exported_at",
    "query",
    "summary",
    "bundles",
    "total_filtered",
    "attestation",
    "checkpoint",
    "export_hash",
    "signature"
  ],
  "properties": {
    "correlation_id": {
      "type": "string",
      "minLength": 1
    },
    "partner_id": {
      "type": "string",
      "minLength": 1
    },
    "integration_mode": {
      "type": "string",
      "enum": [
        "fixture_only"
      ]
    },
    "exported_at": {
      "type": "string",
      "format": "date-time"
    },
    "query": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "milestone_id": {
          "type": "string",
          "minLength": 1
        },
        "environment": {
          "type": "string",
          "enum": [
            "staging"
          ]
        },
        "from_iso": {
          "type": "string",
          "format": "date-time"
        },
        "to_iso": {
          "type": "string",
          "format": "date-time"
        },
        "limit": {
          "type": "integer",
          "minimum": 1,
          "maximum": 200
        },
        "cursor_after": {
          "type": "string",
          "minLength": 1
        },
        "attestation_after": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "checkpoint_after": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "now_iso": {
          "type": "string",
          "format": "date-time"
        },
        "exported_at_iso": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "summary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "total_bundles",
        "returned_bundles",
        "total_evidence_items",
        "returned_evidence_items",
        "latest_checkpoint_hash",
        "by_milestone"
      ],
      "properties": {
        "total_bundles": {
          "type": "integer",
          "minimum": 0
        },
        "returned_bundles": {
          "type": "integer",
          "minimum": 0
        },
        "total_evidence_items": {
          "type": "integer",
          "minimum": 0
        },
        "returned_evidence_items": {
          "type": "integer",
          "minimum": 0
        },
        "latest_checkpoint_hash": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "by_milestone": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "milestone_id",
              "count"
            ],
            "properties": {
              "milestone_id": {
                "type": "string",
                "minLength": 1
              },
              "count": {
                "type": "integer",
                "minimum": 0
              }
            }
          }
        }
      }
    },
    "bundles": {
      "type": "array",
      "items": {
        "$ref": "StagingEvidenceBundleRecordResponse.schema.json#/properties/bundle"
      }
    },
    "total_filtered": {
      "type": "integer",
      "minimum": 0
    },
    "next_cursor": {
      "type": [
        "string",
        "null"
      ]
    },
    "attestation": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "cursor_after",
        "next_cursor",
        "attestation_after",
        "page_hash",
        "chain_hash"
      ],
      "properties": {
        "cursor_after": {
          "type": [
            "string",
            "null"
          ]
        },
        "next_cursor": {
          "type": [
            "string",
            "null"
          ]
        },
        "attestation_after": {
          "type": [
            "string",
            "null"
          ]
        },
        "page_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "chain_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "checkpoint": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "checkpoint_after",
        "attestation_chain_hash",
        "next_cursor",
        "entries_count",
        "total_filtered",
        "checkpoint_hash"
      ],
      "properties": {
        "checkpoint_after": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "attestation_chain_hash": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "next_cursor": {
          "type": [
            "string",
            "null"
          ]
        },
        "entries_count": {
          "type": "integer",
          "minimum": 0
        },
        "total_filtered": {
          "type": "integer",
          "minimum": 0
        },
        "checkpoint_hash": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        }
      }
    },
    "export_hash": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$"
    },
    "signature": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "key_id",
        "alg",
        "sig"
      ],
      "properties": {
        "key_id": {
          "type": "string",
          "minLength": 1
        },
        "alg": {
          "type": "string",
          "minLength": 1
        },
        "sig": {
          "type": "string",
          "minLength": 1
        }
      }
    }
  }
}
